# Environment
ENV=dev  # dev or prod

# API Keys
MISTRAL_API_KEY=your_mistral_api_key_here

# Logging
LOG_LEVEL=INFO  # Will be overridden by environment-specific settings

# Custom Domain (for prod)
DOMAIN=yourdomain.com  # Used in CORS settings for prod environment

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=100  # Will be overridden by environment-specific settings

# Timeouts
API_TIMEOUT=30  # Will be overridden by environment-specific settings 